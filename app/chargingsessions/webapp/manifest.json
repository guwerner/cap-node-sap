{
    "_version": "1.32.0",
    "sap.app": {
        "id": "emobility.cpo.chargingsessions",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/service/risk/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.98.0",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {}
            }
        },
        "models": {
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "TransactionsList",
                    "target": "TransactionsList"
                },
                {
                    "pattern": "Transactions({key}):?query:",
                    "name": "TransactionsObjectPage",
                    "target": "TransactionsObjectPage"
                }
            ],
            "targets": {
                "TransactionsList": {
                    "type": "Component",
                    "id": "TransactionsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Transactions",
                            "variantManagement": "Page",
                            "navigation": {
                                "Transactions": {
                                    "detail": {
                                        "route": "TransactionsObjectPage"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "actions":{
                                        "actionName" : {
                                            "test": "test action",
                                            "visible": true,
                                            "enable": false 
                                        }      
                                    },
                                    "tableSettings": {
                                        "type": "ResponsiveTable",
                                        "quickVariantSelection": {
                                            "paths": [
                                                {
                                                    "key": "tab1",
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#DoneFilter"
                                                },
                                                {
                                                    "key": "tab2",
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#InProcessFilter"
                                                },
                                                {
                                                    "key": "tab3",
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#ErrorFilter"
                                                }
                                            ],
                                            "showCounts": true,
                                            "hideTableTitle": true,
                                            "contextualWidth": true
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "TransactionsObjectPage": {
                    "type": "Component",
                    "id": "TransactionsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": true,
                            "entitySet": "Transactions"
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.cloud": {
        "public": true,
        "service": "cpapp.service"
    }
}